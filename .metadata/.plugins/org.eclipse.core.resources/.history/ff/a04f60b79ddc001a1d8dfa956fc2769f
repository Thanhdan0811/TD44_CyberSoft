
public class BT_for_In_chan_le {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.println("Các số nguyen chẵn < 100 " );
		for(int i = 0; i < 100; i += 2) {
			System.out.print(i + " ");
		}
		System.out.println("Các số nguyen lẻ < 100 " );
		for(int i = 1; i < 100; i += 2) {
			System.out.print(i + " ");
		}
	}

}
