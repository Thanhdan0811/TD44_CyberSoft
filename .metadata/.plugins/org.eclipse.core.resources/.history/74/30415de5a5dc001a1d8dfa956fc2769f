import java.util.Scanner;

public class Demo_ARRAY {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * Khai báo 1 mảng
		 */
		int mangInt[];
		float mangFloat[];
		Scanner mangScan[];
		
		/*
		 * Cấp phát vùng nhớ
		 */
		mangInt = new int[50];
		mangFloat=  new float[20];
		mangScan = new Scanner[50];
	}

}
